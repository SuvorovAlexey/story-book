{
  "name": "story-book",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "storybook": "ng run story-book:storybook",
    "build-storybook": "ng run story-book:build-storybook",
    "build-library": "ng build suvorov-test-ui-lib",
    "build-publish-library-patch": "cd projects/suvorov-test-ui-lib && npm version patch && cd ../../ && ng build suvorov-test-ui-lib && cd dist/suvorov-test-ui-lib && npm publish",
    "build-publish-library-minor": "cd projects/suvorov-test-ui-lib && npm version minor && cd ../../ && ng build suvorov-test-ui-lib && cd dist/suvorov-test-ui-lib && npm publish",
    "build-publish-library-major": "cd projects/suvorov-test-ui-lib && npm version major && cd ../../ && ng build suvorov-test-ui-lib && cd dist/suvorov-test-ui-lib && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@primeng/themes": "^19.0.10",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.3",
    "@angular/cli": "^19.2.3",
    "@angular/compiler-cli": "^19.2.0",
    "@chromatic-com/storybook": "^3.2.6",
    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^8.6.10",
    "@storybook/addon-essentials": "^8.6.10",
    "@storybook/addon-interactions": "^8.6.10",
    "@storybook/addon-onboarding": "^8.6.10",
    "@storybook/angular": "^8.6.10",
    "@storybook/blocks": "^8.6.10",
    "@storybook/test": "^8.6.10",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "storybook": "^8.6.10",
    "typescript": "~5.7.2"
  }
}
